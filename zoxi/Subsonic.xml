<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Name>ZoXi-subsonic</Name>
  <Description>
	
	
  </Description>
  <Registry>https://registry.hub.docker.com/u/zoxi/contenair-subsonic/</Registry>
  <GitHub>https://github.com/devzoxibox/contenair-subsonic</GitHub>
  <Repository>zoxi/contenair-subsonic</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>host</Mode>
    </Networking>
  <Environment>
    <Variable>
      <Name>SSL</Name>
      <Value>yes</Value>
    </Variable>
  </Environment>    
  <Data>
    <Volume>
      <HostDir>path to config</HostDir>
      <ContainerDir>/subsonic</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>path to media</HostDir>
      <ContainerDir>/music</ContainerDir>
      <Mode>ro</Mode>
    </Volume>  	
  </Data>
  <WebUI>https://[IP]:[PORT:4050]/</WebUI>
  <Banner>Contenair Subsonic sous Debian
  	Version : 5.2.1
  	Codecs fonctionels ainsi que la lecture des fichiers "flac"
    </Banner>
  <Icon>http://ericsembrat.com/wp-content/uploads/2012/06/subsonic.png</Icon>
</Containers>
