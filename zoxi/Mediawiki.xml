<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Name>ZoXi-mediawiki</Name>
  <Description>
  
  </Description>
  <Registry>https://registry.hub.docker.com/u/zoxi/container-mediawiki/</Registry>
  <GitHub>https://github.com/devzoxibox/container-mediawiki</GitHub>
  <Repository>zoxi/container-mediawiki</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8081</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>    
  <Data>
    <Volume>
      <HostDir>path to config</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>path to mysql</HostDir>
      <ContainerDir>/var/lib/mysql</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
   </Data>
  <WebUI>http://[IP]:[PORT:8081]/mediawiki</WebUI>
  <Banner></Banner>
  <Icon></Icon>
</Containers>
